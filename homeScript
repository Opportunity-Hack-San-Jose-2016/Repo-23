<?php
$the_query = new WP_Query( 'tag=work2future' );

if ( $the_query->have_posts() ) {
    echo '<ul>';
    while ( $the_query->have_posts() ) {
        $the_query->the_post();
        echo '<li>' . get_the_content() . '</li>';
    }
    echo '</ul>';
} else {
     echo " no posts found";
}
/* Restore original Post Data */
wp_reset_postdata();
?>
